    "import theano\n",
    "import theano.tensor as tt\n",
    "import numpy as np\n",
    "\n",
    "y = tt.scalar(dtype=theano.config.floatX)\n",
    "x = tt.grad(y*tt.log(y), y)\n",
    "test_val = 1e-300\n",
    "print(x.eval({y: test_val}))\n",
    "assert x.eval({y: test_val}) == np.log(test_val)+1"
   ]
  },
